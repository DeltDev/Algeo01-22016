package SPLTuples;
import LinearAlgebra.*;
public class SPLTuples {
	Matrix Mat;
	double [] Solution;
	boolean isSolvable;
	boolean isParametric;
	
	public SPLTuples (Matrix m, double [] sol, boolean solvable, boolean para){
		this.Mat = m;
		this.Solution = sol;
		this.isSolvable = solvable;
		this.isParametric = para;
	}
}
